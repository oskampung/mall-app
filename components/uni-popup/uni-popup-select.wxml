<view class="uni-popup-share data-v-5646ff03"><view class="uni-share-title data-v-5646ff03"><text class="uni-share-title-text data-v-5646ff03">{{title}}</text></view><view class="uni-share-content data-v-5646ff03"><scroll-view class="uni-share-content data-v-5646ff03" scroll-y="true"><block wx:for="{{dicData}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',['$0',index],[[['dicData','',index]]]]]]]}}" class="list data-v-5646ff03" catchtap="__e"><view class="row data-v-5646ff03"><view class="carrier data-v-5646ff03"><view class="left data-v-5646ff03"><view class="title data-v-5646ff03">{{''+row.name}}</view><view class="time data-v-5646ff03">{{"过期时间："+row.expiredTime}}</view><view class="gap-top data-v-5646ff03"></view><view class="gap-bottom data-v-5646ff03"></view></view><view class="right data-v-5646ff03" style="{{'background:'+(row.discount>0?'#bf0f0a':'#eee')+';'}}"><view class="ticket data-v-5646ff03"><view class="num data-v-5646ff03">{{''+row.value}}</view><view class="unit data-v-5646ff03">{{''+(row.type==0?'元':'折')}}</view></view><view class="criteria data-v-5646ff03">{{item['s'+row.stint]}}</view></view></view></view></view></block></scroll-view></view><block wx:if="{{closeBtn}}"><view class="uni-share-button-box data-v-5646ff03"><button data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="uni-share-button data-v-5646ff03" bindtap="__e">取消</button></view></block></view>